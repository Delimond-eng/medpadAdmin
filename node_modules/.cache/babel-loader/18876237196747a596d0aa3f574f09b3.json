    next({ name: 'login' }); // go to '/login';\n    }\n}\n\nexport default new Router({\n    routes: [\n        {\n            path: \"/home\",\n            name:\"home\",\n            beforeEnter: autorization,\n            component: index,\n            children:[\n                {\n                    path: '',\n                    name:'home',\n                    component: home\n                },\n                {\n                    path:'/create/services',\n                    component: servicesPage,\n                    name:\"services\"\n                },\n                {\n                    path:'/create/project',\n                    component: projectPage,\n                    name:\"projects\"\n                },\n                {\n                    path:'/create/partener',\n                    component:partenerPage,\n                    name:\"parteners\",\n                },\n                {\n                    path:'/personalization',\n                    component:personalizationPage,\n                    name:'config'\n                }\n            ]\n        },\n        {\n            path:'/',\n            component:login\n        },\n        //redirect to login\n        { path: '*', redirect: '/' }\n    ]\n});"]}]}