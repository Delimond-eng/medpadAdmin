 }\n    },\n    projects: [],\n    services: [],\n    contacts: [],\n    avantages: [],\n    partenaires: []\n\n};\n\n//all getters\nconst getters = {\n    getProjects: state => state.projects,\n    getServices: state => state.services,\n    getContacts: state => state.contacts,\n    getAvantages: state => state.avantages,\n    getPartenaires: state => state.partenaires\n};\n\n//all mutations or setters\nconst mutations = {\n    setServices: (state, data) => state.services = data,\n    setProjects: (state, data) => state.projects = data,\n    setContacts: (state, data) => state.contacts = data,\n    setAvantages: (state, data) => state.avantages = data,\n    setPartenaires: (state, data) => state.partenaires = data,\n};\n\n//all actions or methods\nconst actions = {\n    //creating service\n    create_parteners({ state }, image) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"image\", image);\n            axios.post(`${state.baseURL}/admin/partenaires/ajouter`, formData, state.optionAxios)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    console.log(resp);\n                    if (status === \"success\") {\n                        resolve(resp.data)\n                    } else {\n                        reject(resp.data);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n    create_services({ state }, service) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"titre\", service.title);\n            formData.append(\"description\", service.description);\n            axios.post(`${state.baseURL}/admin/services/ajouter`, formData, this.optionAxios)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    console.log(resp);\n                    if (status === \"success\") {\n                        resolve(resp)\n                    } else {\n                        reject(resp);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n    create_projects({ state }, project) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"titre\", project.title);\n            formData.append(\"description\", project.description);\n            formData.append(\"image\", project.file);\n            axios.post(`${state.baseURL}/admin/projets/publier`, formData, state.optionAxios)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    if (status === \"success\") {\n                        resolve(resp.data)\n                    } else {\n                        reject(resp.data);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n\n    create_realisation({ state }, realisation) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"texte\", realisation.title);\n            formData.append(\"images\", realisation.file);\n            axios.post(`${state.baseURL}/admin/realisations/poster`, formData, state.optionAxios)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    if (status === \"success\") {\n                        resolve(resp.data)\n                    } else {\n                        reject(resp.data);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n    site_config({ state }, options) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"logo\", options.logo);\n            formData.append(\"banner_img1\", options.banner1);\n            formData.append(\"banner_img2\", options.banner2);\n            formData.append(\"banner_img3\", options.banner3);\n            formData.append(\"a_propos\", options.about);\n            formData.append(\"video_presentation\", options.video);\n            axios.post(`${state.baseURL}/admin/personnalisation`, formData, this.optionAxios)\n                .then((resp) => {\n                    let status = resp.data.reponse.status;\n                    if (status === \"success\") {\n                        resolve(resp.data)\n                    } else {\n                        reject(resp.data);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n\n    viewContent({ commit, state }) {\n        try {\n            axios.get(`${state.baseURL}/admin/contenus`)\n                .then((result) => {\n                    commit(\"setProjects\", result.data.contenus.projets);\n                    commit(\"setServices\", result.data.contenus.services);\n                    commit(\"setContacts\", result.data.contenus.contacts);\n                    commit(\"setPartenaires\", result.data.contenus.partenaires);\n\n                })\n                .catch(err => console.log(err));\n\n\n            axios.get(`${state.baseURL}/admin/personnalisation/get`)\n                .then((result) => {\n                    commit(\"setAvantages\", result.data.personalisation.avantages);\n                })\n                .catch(err => console.log(err));\n        } catch (err) {\n            console.log(err)\n        }\n    },\n\n    deleteService({ state }, serviceId) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"service_id\", serviceId);\n            axios.post(`${state.baseURL}/admin/services/supprimer`, formData, state.optionAxios)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    if (status === \"success\") {\n                        resolve(resp.data.reponse.status)\n                    } else {\n                        reject(resp.data);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n\n    deleteProject({ state }, projetId) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"projet_id\", projetId);\n            axios.post(`${state.baseURL}/admin/projets/supprimer`, formData, state.optionAxios)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    if (status === \"success\") {\n                        resolve(resp.data.reponse.status)\n                    } else {\n                        reject(resp.didata);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    }\n};\n\nexport default {\n    state,\n    getters,\n    mutations,\n    actions\n}"]}]}