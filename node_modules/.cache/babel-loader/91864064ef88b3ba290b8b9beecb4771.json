MAAA,KAAA,SAAA,EAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA;AACA,cAAA,KAAA,EAAA,IADA;AAEA,cAAA,iBAAA,EAAA,KAFA;AAGA,cAAA,QAAA,EAAA,SAHA;AAIA,cAAA,KAAA,EAAA,IAJA;AAKA,cAAA,gBAAA,EAAA,KALA;AAMA,cAAA,IAAA,EAAA,SANA;AAOA,cAAA,KAAA,EAAA;AAPA,aAAA;;AASA,YAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA;;AACA,YAAA,MAAA,CAAA,MAAA;AACA;AACA,SAjBA,EAkBA,KAlBA,CAkBA,UAAA,GAAA;AAAA,iBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,SAlBA;AAmBA,OA7BA;AA8BA,KA/FA;AAiGA,IAAA,MAjGA,oBAiGA;AACA,WAAA,QAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,CAAA,WAAA,GAAA,EAAA;AACA;AApGA;AArBA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- Content Header (Page header) -->\n    <div class=\"content-header\">\n      <div class=\"container-fluid\">\n        <div class=\"row mb-2\">\n          <div class=\"col-sm-6\">\n            <h1 class=\"m-0 text-dark\">Personnalisation Avantages</h1>\n          </div>\n          <div class=\"col-sm-6\">\n            <ol class=\"breadcrumb float-sm-right\">\n              <li class=\"breadcrumb-item\">\n                <router-link :to=\"{ name: 'home' }\" tag=\"a\">Acceuil</router-link>\n              </li>\n            </ol>\n          </div>\n          <!-- /.col -->\n        </div>\n        <!-- /.row -->\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content-header -->\n\n    <!-- Main content -->\n    <div class=\"content\">\n      <div class=\"container-fluid\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Ajout avantages</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label for=\"InputTitle\">Titre avantage</label>\n                    <input\n                      type=\"text\"\n                      v-model=\"avantage.title\"\n                      class=\"form-control border-primary\"\n                      id=\"InputTitle\"\n                      placeholder=\"Entrer le titre du service\"\n                    />\n                  </div>\n                  <div class=\"form-group\">\n                    <label>Description avantage</label>\n                    <textarea\n                      class=\"form-control border-primary\"\n                      v-model=\"avantage.description\"\n                      rows=\"3\"\n                      placeholder=\"Entrer la description\"\n                    ></textarea>\n                  </div>\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button type=\"submit\" class=\"btn btn-primary mr-2\" @click=\"create\">\n                  <i class=\"fa fa-plus mr-2\"></i> Ajouter\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <table class=\"table table-striped table-valign-middle\">\n                  <thead>\n                    <tr>\n                      <th>Titre</th>\n                      <th>Description</th>\n                      <th></th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"(data, index) in avantages\" :key=\"index\">\n                      <td>{{ data.titre | sortlength(100, \"...\") }}</td>\n                      <td>\n                        {{ data.description | sortlength(100, \"...\") }}\n                      </td>\n                      <td>\n                        <button\n                          type=\"button\"\n                          class=\"btn btn-sm btn-danger\"\n                          @click=\"onDelete(data.avantage_id)\"\n                        >\n                          <i class=\"fa fa-trash mr-2\"></i>supprimer\n                        </button>\n                      </td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"avantages\",\n  data() {\n    return {\n      avantage: {\n        title: \"\",\n        description: \"\",\n      },\n    };\n  },\n\n  computed: {\n    avantages() {\n      return this.$store.getters.getAvantages;\n    },\n  },\n\n  mounted() {\n    this.$store.dispatch(\"viewContent\");\n  },\n\n  methods: {\n    onDelete(id) {\n      let formData = new FormData();\n      formData.append(\"avantage_id\", id);\n      this.$swal({\n        buttonsStyling: true,\n        title: \"Etes vous sûr?\",\n        text: \"de vouloir supprimer définitivement cet avantage ?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\",\n      }).then((result) => {\n        if (result.value) {\n          this.$axios\n            .post(\"admin/avantages/supprimer\", formData)\n            .then((result) => {\n              let status = result.data.reponse.status;\n\n              if (status === \"success\") {\n                this.$swal({\n                  toast: true,\n                  showConfirmButton: false,\n                  position: \"top-end\",\n                  timer: 2000,\n                  timerProgressBar: false,\n                  icon: \"success\",\n                  title: \"la suppression est effectuée!\",\n                });\n                this.$store.dispatch(\"viewContent\");\n              }\n            })\n            .catch((err) => console.log(err));\n        } else return;\n      });\n    },\n    create() {\n      if (this.avantage.title === \"\") {\n        this.$swal({\n          toast: true,\n          showConfirmButton: false,\n          position: \"top-end\",\n          timer: 2000,\n          timerProgressBar: false,\n          icon: \"warning\",\n          title: \"vous devez entrer le titre du projet !\",\n        });\n        return;\n      }\n\n      if (this.avantage.description === \"\") {\n        this.$swal({\n          toast: true,\n          showConfirmButton: false,\n          position: \"top-end\",\n          timer: 2000,\n          timerProgressBar: false,\n          icon: \"warning\",\n          title: \"vous devez entrer la description du projet !\",\n        });\n        return;\n      }\n      let formData = new FormData();\n      formData.append(\"titre\", this.avantage.title);\n      formData.append(\"description\", this.avantage.description);\n      this.$swal({\n        title: \"\",\n        html: \"Traitement en cours...\",\n        timer: 2000,\n        allowOutsideClick: false,\n        showConfirmButton: false,\n        onBeforeOpen: () => {\n          this.$swal.showLoading();\n        },\n      }).then(() => {\n        this.$axios\n          .post(\"admin/avantages/ajouter\", formData)\n          .then((result) => {\n            let status = result.data.reponse.status;\n            if (status === \"success\") {\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"top-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"success\",\n                title: \"service ajouté avec succès !\",\n              });\n              this.$store.dispatch(\"viewContent\");\n              this.cancel();\n            }\n          })\n          .catch((err) => console.log(err));\n      });\n    },\n\n    cancel() {\n      this.avantage.title = \"\";\n      this.avantage.description = \"\";\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components/pages"}]}