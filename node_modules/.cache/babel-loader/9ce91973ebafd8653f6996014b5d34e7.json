w mb-2\">\n          <div class=\"col-sm-6\">\n            <h1 class=\"m-0 text-dark\">Création projets</h1>\n          </div>\n          <!-- /.col -->\n          <div class=\"col-sm-6\">\n            <ol class=\"breadcrumb float-sm-right\">\n              <li class=\"breadcrumb-item\">\n                <router-link :to=\"{ name: 'home' }\" tag=\"a\">Acceuil</router-link>\n              </li>\n            </ol>\n          </div>\n        </div>\n        <!-- /.row -->\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content-header -->\n\n    <!-- Main content -->\n    <div class=\"content\">\n      <div class=\"container-fluid\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Ajout projet</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" enctype=\"multipart/form-data\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label for=\"InputTitle\">réalisation titre</label>\n                    <input\n                      type=\"email\"\n                      v-model=\"realisation.title\"\n                      class=\"form-control border-primary\"\n                      id=\"InputTitle\"\n                      placeholder=\"Entrer le titre de la réalisation\"\n                    />\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"InputFile\">Images de la réalisation</label>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file1\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage1($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file1 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file1.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 1</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file2\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage2($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file2 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file2.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 2</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file3\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage3($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file3 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file3.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 3</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file4\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage4($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file4 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file4.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 4</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file5\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage5($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file5 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file5.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 5</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file6\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage6($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file6 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file6.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 6</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file7\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage7($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file7 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file7.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 7</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file8\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage8($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file8 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file8.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 8</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file9\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage9($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file9 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file9.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 9</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file10\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage10($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file10 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file10.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 10</label\n                        >\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button type=\"button\" class=\"btn btn-primary mr-2\" @click=\"submitted\">\n                  <i class=\"fa fa-plus mr-2\" v-if=\"isLoad === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm mr-2\"\n                    v-if=\"isLoad === true\"\n                  ></span\n                  >Poster\n                </button>\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <table class=\"table table-striped table-valign-middle\">\n                  <thead>\n                    <tr>\n                      <th>Titre</th>\n                      <th></th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"(data, index) in realisations\" :key=\"index\">\n                      <td>\n                        {{ data.texte }}\n                      </td>\n                      <td>\n                        <button\n                          type=\"button\"\n                          class=\"btn btn-sm btn-danger\"\n                          @click=\"onDelete(data.realisation_id)\"\n                        >\n                          <i class=\"fa fa-trash mr-2\"></i>supprimer\n                        </button>\n                      </td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"realisationComponent\",\n  data() {\n    return {\n      realisation: {\n        title: \"\",\n        file1: \"\",\n        file2: \"\",\n        file3: \"\",\n        file4: \"\",\n        file5: \"\",\n        file6: \"\",\n        file7: \"\",\n        file8: \"\",\n        file9: \"\",\n        file10: \"\",\n      },\n      isUploaded: false,\n      isLoad: false,\n      realisations: [],\n    };\n  },\n\n  mounted() {\n    this.viewRealisations();\n  },\n\n  methods: {\n    submitted() {\n      if (this.isUploaded === false) {\n        this.$swal({\n          toast: true,\n          showConfirmButton: false,\n          position: \"top-end\",\n          timer: 2000,\n          timerProgressBar: false,\n          icon: \"warning\",\n          title: \"vous devez charger lau moins une image de la réalisation !\",\n        });\n        return;\n      }\n      this.isLoad = true;\n      let formData = new FormData();\n      formData.append(\"texte\", this.realisation.title);\n      formData.append(\"image_1\", this.realisation.file1);\n      formData.append(\"image_2\", this.realisation.file2);\n      formData.append(\"image_3\", this.realisation.file3);\n      formData.append(\"image_4\", this.realisation.file4);\n      formData.append(\"image_5\", this.realisation.file5);\n      formData.append(\"image_6\", this.realisation.file6);\n      formData.append(\"image_7\", this.realisation.file7);\n      formData.append(\"image_8\", this.realisation.file8);\n      formData.append(\"image_9\", this.realisation.file9);\n      formData.append(\"image_10\", this.realisation.file10);\n\n      this.$axios\n        .post(\"admin/realisations/poster\", formData)\n        .then((result) => {\n          let status = result.data.reponse.status;\n          console.log(status);\n\n          if (status === \"success\") {\n            this.isLoad = false;\n            this.$swal({\n              showConfirmButton: false,\n              timer: 2000,\n              timerProgressBar: false,\n              icon: \"success\",\n              text: \"Le post de la réalisation est effectué !\",\n            });\n            this.cancel();\n            this.viewRealisations();\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n          this.isLoad = false;\n          this.cancel();\n        });\n    },\n\n    onDelete(id) {\n      let formData = new FormData();\n      formData.append(\"realisation_id\", id);\n      this.$swal({\n        buttonsStyling: true,\n        title: \"Etes vous sûr?\",\n        text: \"de vouloir supprimer définitivement cette realisation ?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\",\n      }).then((result) => {\n        if (result.value) {\n          this.$axios.post(\"admin/realisations/supprimer\", formData).then((result) => {\n            var status = result.data.reponse.status;\n            if (status == \"success\") {\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"top-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"success\",\n                title: \"la suppression est effectuée!\",\n              });\n              this.viewRealisations();\n            } else {\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"bottom-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"warning\",\n                title: \"Echec de la suppression !\",\n              });\n              return;\n            }\n          });\n        } else return;\n      });\n    },\n\n    viewRealisations() {\n      try {\n        this.$axios\n          .get(\"admin/realisations/voir\")\n          .then((result) => {\n            this.realisations = result.data.realisations;\n          })\n          .catch((err) => console.log(err));\n      } catch (e) {\n        console.log(e);\n      }\n    },\n\n    cancel() {\n      this.realisation.title = \"\";\n      this.realisation.image = \"\";\n      this.realisation.file1 = \"\";\n      this.realisation.file2 = \"\";\n      this.realisation.file3 = \"\";\n      this.realisation.file4 = \"\";\n      this.realisation.file5 = \"\";\n      this.realisation.file6 = \"\";\n      this.realisation.file7 = \"\";\n      this.realisation.file8 = \"\";\n      this.realisation.file9 = \"\";\n      this.realisation.file10 = \"\";\n      this.isUploaded = false;\n    },\n\n    uploadImage1() {\n      this.realisation.file1 = this.$refs.file1.files[0];\n      console.log(this.realisation.file1);\n      this.isUploaded = true;\n    },\n    uploadImage2() {\n      this.realisation.file2 = this.$refs.file2.files[0];\n      console.log(this.realisation.file2);\n      this.isUploaded = true;\n    },\n\n    uploadImage3() {\n      this.realisation.file3 = this.$refs.file3.files[0];\n      console.log(this.realisation.file3);\n      this.isUploaded = true;\n    },\n\n    uploadImage4() {\n      this.realisation.file4 = this.$refs.file4.files[0];\n      console.log(this.realisation.file4);\n      this.isUploaded = true;\n    },\n    uploadImage5() {\n      this.realisation.file5 = this.$refs.file5.files[0];\n      console.log(this.realisation.file5);\n      this.isUploaded = true;\n    },\n    uploadImage6() {\n      this.realisation.file6 = this.$refs.file6.files[0];\n      console.log(this.realisation.file6);\n      this.isUploaded = true;\n    },\n\n    uploadImage7() {\n      this.realisation.file7 = this.$refs.file7.files[0];\n      this.isUploaded = true;\n    },\n\n    uploadImage8() {\n      this.realisation.file8 = this.$refs.file8.files[0];\n      this.isUploaded = true;\n    },\n\n    uploadImage9() {\n      this.realisation.file9 = this.$refs.file9.files[0];\n      this.isUploaded = true;\n    },\n    uploadImage10() {\n      this.realisation.file10 = this.$refs.file10.files[0];\n      this.isUploaded = true;\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}