n                    reject(err)\n                })\n        })\n    },\n    create_services({ state }, service) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"titre\", service.title);\n            formData.append(\"description\", service.description);\n            axios.post(`${state.baseURL}/admin/services/ajouter`, formData)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    console.log(resp);\n                    if (status === \"success\") {\n                        resolve(resp)\n                    } else {\n                        reject(resp);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n    create_projects({ state }, project) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"titre\", project.title);\n            formData.append(\"description\", project.description);\n            formData.append(\"image\", project.image);\n            axios.post(`${state.baseURL}/admin/projets/publier`, formData)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    console.log(resp);\n                    if (status === \"success\") {\n                        resolve(resp)\n                    } else {\n                        reject(resp);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n    siteconfig({ state }, options) {\n        return new Promise((resolve, reject) => {\n            let formData = new FormData();\n            formData.append(\"logo\", options.logo);\n            formData.append(\"banner_img1\", options.banner1);\n            formData.append(\"banner_img2\", options.banner2);\n            formData.append(\"banner_img3\", options.banner3);\n            formData.append(\"a_propos\", options.about);\n            formData.append(\"video_presentation\", options.video);\n            axios.post(`${state.baseURL}/admin/personnalisation`, formData)\n                .then((resp) => {\n                    const status = resp.data.reponse.status;\n                    console.log(resp);\n                    if (status === \"success\") {\n                        resolve(resp)\n                    } else {\n                        reject(resp);\n                    }\n                })\n                .catch((err) => {\n                    reject(err)\n                })\n        })\n    },\n};\n\nexport default {\n    state,\n    getters,\n    mutations,\n    actions\n}"]}]}