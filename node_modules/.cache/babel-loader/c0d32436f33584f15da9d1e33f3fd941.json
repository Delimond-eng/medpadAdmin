A,IAAA,SAAA,EAAA;AACA,cAAA,MAAA,CAAA,KAAA,CAAA;AACA,gBAAA,KAAA,EAAA,IADA;AAEA,gBAAA,iBAAA,EAAA,KAFA;AAGA,gBAAA,QAAA,EAAA,SAHA;AAIA,gBAAA,KAAA,EAAA,IAJA;AAKA,gBAAA,gBAAA,EAAA,KALA;AAMA,gBAAA,IAAA,EAAA,SANA;AAOA,gBAAA,KAAA,EAAA;AAPA,eAAA;;AASA,cAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA;AACA,aAXA,MAWA;AACA,cAAA,MAAA,CAAA,KAAA,CAAA;AACA,gBAAA,KAAA,EAAA,IADA;AAEA,gBAAA,iBAAA,EAAA,KAFA;AAGA,gBAAA,QAAA,EAAA,YAHA;AAIA,gBAAA,KAAA,EAAA,IAJA;AAKA,gBAAA,gBAAA,EAAA,KALA;AAMA,gBAAA,IAAA,EAAA,SANA;AAOA,gBAAA,KAAA,EAAA;AAPA,eAAA;;AASA;AACA;AACA,WAzBA;AA0BA,SA3BA,MA2BA;AACA,OArCA;AAsCA,KAvFA;AAyFA,IAAA,WAzFA,uBAyFA,KAzFA,EAyFA;AAAA;;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,UAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;;AAEA,UAAA,IAAA,CAAA,IAAA,GAAA,OAAA,EAAA;AACA,QAAA,KAAA,CAAA,kCAAA,CAAA;AACA,OAFA,MAEA;AACA,YAAA,MAAA,GAAA,IAAA,UAAA,EAAA;;AAEA,QAAA,MAAA,CAAA,MAAA,GAAA,UAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,KAAA,CAAA,MAAA,CAAA,MAAA;AACA,SAFA;;AAGA,QAAA,MAAA,CAAA,aAAA,CAAA,IAAA;AACA,aAAA,UAAA,GAAA,IAAA;AACA;AACA,KAxGA;AA0GA,IAAA,MA1GA,oBA0GA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,UAAA,GAAA,KAAA;AACA;AA9GA;AAvBA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- Content Header (Page header) -->\n    <div class=\"content-header\">\n      <div class=\"container-fluid\">\n        <div class=\"row mb-2\">\n          <div class=\"col-sm-6\">\n            <h1 class=\"m-0 text-dark\">Création partenaires</h1>\n          </div>\n          <div class=\"col-sm-6\">\n            <ol class=\"breadcrumb float-sm-right\">\n              <li class=\"breadcrumb-item\">\n                <router-link :to=\"{ name: 'home' }\" tag=\"a\">Acceuil</router-link>\n              </li>\n            </ol>\n          </div>\n          <!-- /.col -->\n        </div>\n        <!-- /.row -->\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content-header -->\n\n    <!-- Main content -->\n    <div class=\"content\">\n      <div class=\"container-fluid\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Ajout partenaires</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form enctype=\"multipart/form-data\" ref=\"coverForm\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label for=\"InputFile\">Image de couverture</label>\n                    <div class=\"input-group\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          class=\"custom-file-input border-primary\"\n                          multiple=\"multiple\"\n                          @change=\"uploadImage($event)\"\n                          ref=\"file\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"form.file !== ''\"\n                          class=\"custom-file-label\"\n                          for=\"InputFile\"\n                          >{{ form.file.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image</label\n                        >\n                      </div>\n                    </div>\n                  </div>\n\n                  <!--Logo view-->\n                  <div class=\"row\" v-if=\"isUploaded\">\n                    <div class=\"col-sm-3\">\n                      <img class=\"img-fluid mb-3\" width=\"200\" :src=\"image\" alt=\"Photo\" />\n                    </div>\n                  </div>\n                  <!--Logo view end-->\n                </div>\n                <!-- /.card-body -->\n              </form>\n\n              <div class=\"card-footer\">\n                <button type=\"submit\" @click=\"add\" class=\"btn btn-primary mr-2\">\n                  <i class=\"fa fa-plus mr-2\"></i> Ajouter\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <!--End row-->\n\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h2 class=\"card-title\">Partenaires</h2>\n              </div>\n              <div class=\"card-body\">\n                <div class=\"row\">\n                  <div class=\"col-md-2\" v-for=\"(data, index) in partenaires\" :key=\"index\">\n                    <div>\n                      <img\n                        class=\"img-fluid m-1\"\n                        :src=\"data.image\"\n                        style=\"\n                          width: 100%;\n                          height: 200px;\n                          object-fit: cover;\n                          object-position: center;\n                        \"\n                      />\n                      <button\n                        class=\"btn btn-danger\"\n                        style=\"\n                          position: absolute;\n                          z-index: 1000;\n                          left: 0;\n                          border-radius: 50px;\n                          right: 0;\n                          top: 80px;\n                          margin-left: auto;\n                          margin-right: auto;\n                        \"\n                        @click=\"onDelete(data.partenaire_id)\"\n                      >\n                        supprimer\n                      </button>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"partener\",\n  data() {\n    return {\n      image: \"\",\n      isUploaded: false,\n      isLoading: false,\n      form: {\n        file: \"\",\n      },\n    };\n  },\n\n  mounted() {\n    this.$store.dispatch(\"viewContent\");\n  },\n\n  computed: {\n    partenaires() {\n      return this.$store.getters.getPartenaires;\n    },\n  },\n\n  methods: {\n    add() {\n      if (this.form.file === \"\") {\n        this.$swal({\n          toast: true,\n          showConfirmButton: false,\n          position: \"top-end\",\n          timer: 2000,\n          timerProgressBar: false,\n          icon: \"warning\",\n          title: \"vous devez charger le logo du partenaire !\",\n        });\n      } else {\n        this.$swal({\n          title: \"Chargement\",\n          html: \"Traitement en cours...\",\n          timer: 2000,\n          allowOutsideClick: false,\n          showConfirmButton: false,\n          onBeforeOpen: () => {\n            this.$swal.showLoading();\n          },\n        }).then(() => {\n          console.log(this.form.file);\n          this.$store\n            .dispatch(\"create_parteners\", this.form.file)\n            .then((result) => {\n              console.log(result);\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"top-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"success\",\n                title: \"partenaire ajouté avec succès !\",\n              });\n              this.cancel();\n              this.$store.dispatch(\"viewContent\");\n            })\n            .catch((err) => console.log(err));\n        });\n      }\n    },\n\n    onDelete(id) {\n      console.log(id);\n      let formData = new FormData();\n      formData.append(\"partenaire_id\", id);\n      this.$swal({\n        buttonsStyling: true,\n        title: \"Etes vous sûr?\",\n        text: \"de vouloir supprimer définitivement ce partenaire ?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\",\n      }).then((result) => {\n        if (result.value) {\n          this.$axios.post(\"admin/partenaires/supprimer\", formData).then((result) => {\n            var status = result.data.reponse.status;\n            if (status == \"success\") {\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"top-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"success\",\n                title: \"la suppression est effectuée!\",\n              });\n              this.$store.dispatch(\"viewContent\");\n            } else {\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"bottom-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"warning\",\n                title: \"Echec de la suppression !\",\n              });\n              return;\n            }\n          });\n        } else return;\n      });\n    },\n\n    uploadImage(event) {\n      this.form.file = this.$refs.file.files[0];\n      let file = event.target.files[0];\n\n      if (file.size > 1048576) {\n        alert(\"Desolé! cette image est invalide\");\n      } else {\n        let reader = new FileReader();\n\n        reader.onload = (event) => {\n          this.image = event.target.result;\n        };\n        reader.readAsDataURL(file);\n        this.isUploaded = true;\n      }\n    },\n\n    cancel() {\n      this.form.file = \"\";\n      this.image = \"\";\n      this.isUploaded = false;\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}