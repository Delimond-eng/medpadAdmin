      <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Personnalisation logo</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" enctype=\"multipart/form-data\" ref=\"settingsForm\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label for=\"logo\">Logo (optionnel)</label>\n                    <div class=\"input-group\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"logoFile\"\n                          multiple=\"multiple\"\n                          class=\"custom-file-input border-primary\"\n                          id=\"logo\"\n                          @change=\"uploadLogo($event)\"\n                        />\n                        <label class=\"custom-file-label\" for=\"logo\">Choisir image</label>\n                      </div>\n                    </div>\n                  </div>\n                  <!--Logo view-->\n                  <div class=\"row\" v-if=\"checker.isLogo\">\n                    <div class=\"col-sm-3\">\n                      <img\n                        class=\"img-fluid mb-3\"\n                        width=\"200\"\n                        :src=\"viewer.imgLogo\"\n                        alt=\"Photo\"\n                      />\n                    </div>\n                  </div>\n                  <!--video view end-->\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button\n                  type=\"submit\"\n                  class=\"btn btn-primary mr-2\"\n                  @click=\"submitted('logo', options.logo)\"\n                >\n                  <i class=\"fa fa-cog mr-2\" v-if=\"loading === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm\"\n                    v-if=\"loading === true\"\n                  ></span>\n                  Personnaliser\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <!--Logo End-->\n\n        <!--Banners-->\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Personnalisation bannières</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" enctype=\"multipart/form-data\" ref=\"settingsForm\">\n                <div class=\"card-body\">\n                  <div class=\"row\">\n                    <div class=\"col-lg-4 col-md-6 col-12\">\n                      <div class=\"form-group\">\n                        <label for=\"banner1\">Bannière 1 (optionnel)</label>\n                        <div class=\"input-group\">\n                          <div class=\"custom-file\">\n                            <input\n                              type=\"file\"\n                              ref=\"bannerFile1\"\n                              multiple=\"multiple\"\n                              @change=\"uploadBanne1($event)\"\n                              class=\"custom-file-input border-primary\"\n                              id=\"banner1\"\n                            />\n                            <label class=\"custom-file-label\" for=\"banner1\"\n                              >Choisir image</label\n                            >\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"col-lg-4 col-md-6 col-12\">\n                      <div class=\"form-group\">\n                        <label for=\"banner1\">Bannière 2 (optionnel)</label>\n                        <div class=\"input-group\">\n                          <div class=\"custom-file\">\n                            <input\n                              type=\"file\"\n                              class=\"custom-file-input border-primary\"\n                              ref=\"bannerFile2\"\n                              @change=\"uploadBanne2($event)\"\n                              multiple=\"multiple\"\n                              id=\"banner2\"\n                            />\n                            <label class=\"custom-file-label\" for=\"banner2\"\n                              >Choisir image</label\n                            >\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"col-lg-4 col-md-6 col-12\">\n                      <div class=\"form-group\">\n                        <label for=\"banner3\">Bannière 3 (optionnel)</label>\n                        <div class=\"input-group\">\n                          <div class=\"custom-file\">\n                            <input\n                              type=\"file\"\n                              class=\"custom-file-input border-primary\"\n                              ref=\"bannerFile3\"\n                              multiple=\"multiple\"\n                              @change=\"uploadBanne3($event)\"\n                              id=\"banner3\"\n                            />\n                            <label class=\"custom-file-label\" for=\"banner2\"\n                              >Choisir image</label\n                            >\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                  <!--banner view-->\n                  <div class=\"row\">\n                    <div class=\"col-lg-4 col-md-6 col-12\" v-if=\"checker.isBanner1\">\n                      <img class=\"img-fluid mb-3\" :src=\"viewer.imgBan1\" alt=\"Photo\" />\n                    </div>\n                    <div class=\"col-lg-4 col-md-6 col-12\" v-if=\"checker.isBanner2\">\n                      <img class=\"img-fluid mb-3\" :src=\"viewer.imgBan2\" alt=\"Photo\" />\n                    </div>\n                    <div class=\"col-lg-4 col-md-6 col-12\" v-if=\"checker.isBanner3\">\n                      <img class=\"img-fluid mb-3\" :src=\"viewer.imgBan3\" alt=\"Photo\" />\n                    </div>\n                    <!-- /.col -->\n                  </div>\n                  <!--banner view end-->\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button type=\"submit\" class=\"btn btn-danger mr-2\" @click=\"saveBanner\">\n                  <i class=\"fa fa-cog mr-2\" v-if=\"loading === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm\"\n                    v-if=\"loading === true\"\n                  ></span>\n                  Personnaliser\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <!--Banners End-->\n\n        <!--About-->\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Personnalisation à propos</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" enctype=\"multipart/form-data\" ref=\"settingsForm\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label>A propos (optionnel)</label>\n                    <textarea\n                      class=\"form-control border-primary\"\n                      v-model=\"options.about\"\n                      rows=\"3\"\n                      placeholder=\"Entrer la description\"\n                    ></textarea>\n                  </div>\n                  <!--video view end-->\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button\n                  type=\"submit\"\n                  class=\"btn btn-warning mr-2\"\n                  @click=\"submitted('a_propos', options.about)\"\n                >\n                  <i class=\"fa fa-cog mr-2\" v-if=\"loading === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm\"\n                    v-if=\"loading === true\"\n                  ></span>\n                  Personnaliser\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <!--About End-->\n\n        <!--Mission-->\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Personnalisation mission</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label>Mission</label>\n                    <textarea\n                      class=\"form-control border-primary\"\n                      v-model=\"options.mission\"\n                      rows=\"3\"\n                      placeholder=\"Entrer la description\"\n                    ></textarea>\n                  </div>\n                  <!--video view end-->\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button\n                  type=\"submit\"\n                  class=\"btn btn-dark mr-2\"\n                  @click=\"submitted('mission', options.mission)\"\n                >\n                  <i class=\"fa fa-cog mr-2\" v-if=\"loading === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm\"\n                    v-if=\"loading === true\"\n                  ></span>\n                  Personnaliser\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <!--Mission End-->\n\n        <!--Youtube Link-->\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Personnalisation des liens de nos vidéos</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" ref=\"settingsForm\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label for=\"banner3\">Lien de la vidéo</label>\n                    <input\n                      type=\"text\"\n                      v-model=\"options.videoLink\"\n                     