         <div class=\"card-footer\">\n                <button\n                  type=\"submit\"\n                  class=\"btn btn-primary mr-2\"\n                  @click=\"submitted('client_satisfait', options.client_satisfait)\"\n                >\n                  <i class=\"fa fa-people-carry mr-2\" v-if=\"loading === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm\"\n                    v-if=\"loading === true\"\n                  ></span>\n                  Personnaliser\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <!--Clients Satisfaits End-->\n\n        <!--Clients satisfaits cover-->\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">\n                  Personnalisation image de fond de la section clients satisfaits\n                </h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" enctype=\"multipart/form-data\" ref=\"settingsForm\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label for=\"client\">image</label>\n                    <div class=\"input-group\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"clientFile\"\n                          multiple=\"multiple\"\n                          class=\"custom-file-input border-primary\"\n                          id=\"client\"\n                          @change=\"uploadCover($event)\"\n                        />\n                        <label class=\"custom-file-label\" for=\"client\"\n                          >Choisir image</label\n                        >\n                      </div>\n                    </div>\n                  </div>\n                  <!--Logo view-->\n                  <div class=\"row\" v-if=\"checker.isClientCover\">\n                    <div class=\"col-sm-3\">\n                      <img\n                        class=\"img-fluid mb-3\"\n                        width=\"200\"\n                        :src=\"viewer.client_cover\"\n                        alt=\"Photo\"\n                      />\n                    </div>\n                  </div>\n                  <!--video view end-->\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button\n                  type=\"submit\"\n                  class=\"btn btn-success mr-2\"\n                  @click=\"submitted('client_satisfait_cover', options.cover_file)\"\n                >\n                  <i class=\"fa fa-cog mr-2\" v-if=\"loading === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm\"\n                    v-if=\"loading === true\"\n                  ></span>\n                  Personnaliser\n                </button>\n                <button type=\"submit\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <!--Clients Satisfaits End-->\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"personalization\",\n  data() {\n    return {\n      loading: false,\n      checker: {\n        isLogo: false,\n        isBanner1: false,\n        isBanner2: false,\n        isBanner3: false,\n        isClientCover: false,\n      },\n      viewer: {\n        imgLogo: \"\",\n        imgBan1: \"\",\n        imgBan2: \"\",\n        imgBan3: \"\",\n        client_cover: \"\",\n      },\n      options: {\n        logo: \"\",\n        banner1: \"\",\n        banner2: \"\",\n        banner3: \"\",\n        about: \"\",\n        videoLink: \"\",\n        client_satisfait: \"\",\n        cover_file: \"\",\n        mission: \"\",\n      },\n    };\n  },\n\n  methods: {\n    uploadLogo() {\n      this.options.logo = this.$refs.logoFile.files[0];\n      let file = event.target.files[0];\n\n      if (file.size > 5048576) {\n        alert(\"Desolé! la taille de l'image est invalide\");\n      } else {\n        let reader = new FileReader();\n\n        reader.onload = (event) => {\n          this.viewer.imgLogo = event.target.result;\n        };\n        reader.readAsDataURL(file);\n        this.checker.isLogo = true;\n      }\n    },\n    uploadBanne1(event) {\n      this.options.banner1 = this.$refs.bannerFile1.files[0];\n      let file = event.target.files[0];\n\n      if (file.size > 11048576) {\n        alert(\"Desolé! la taille de l'image est invalide\");\n      } else {\n        let reader = new FileReader();\n\n        reader.onload = (event) => {\n          this.viewer.imgBan1 = event.target.result;\n        };\n        reader.readAsDataURL(file);\n        this.checker.isBanner1 = true;\n      }\n    },\n    uploadBanne2(event) {\n      this.options.banner2 = this.$refs.bannerFile2.files[0];\n      let file = event.target.files[0];\n\n      if (file.size > 11048576) {\n        alert(\"Desolé! la taille de l'image est invalide\");\n      } else {\n        let reader = new FileReader();\n\n        reader.onload = (event) => {\n          this.viewer.imgBan2 = event.target.result;\n        };\n        reader.readAsDataURL(file);\n        this.checker.isBanner2 = true;\n      }\n    },\n    uploadBanne3(event) {\n      this.options.banner3 = this.$refs.bannerFile3.files[0];\n      let file = event.target.files[0];\n\n      if (file.size > 11048576) {\n        alert(\"Desolé! la taille de l'image est invalide\");\n      } else {\n        let reader = new FileReader();\n\n        reader.onload = (event) => {\n          this.viewer.imgBan3 = event.target.result;\n        };\n        reader.readAsDataURL(file);\n        this.checker.isBanner3 = true;\n      }\n    },\n\n    uploadCover() {\n      this.options.cover_file = this.$refs.clientFile.files[0];\n      let file = event.target.files[0];\n\n      if (file.size > 11048576) {\n        alert(\"Desolé! la taille de l'image est invalide\");\n      } else {\n        let reader = new FileReader();\n\n        reader.onload = (event) => {\n          this.viewer.client_cover = event.target.result;\n        };\n        reader.readAsDataURL(file);\n        this.checker.isClientCover = true;\n      }\n    },\n\n    cancel() {\n      this.checker.isBanner1 = false;\n      this.checker.isBanner2 = false;\n      this.checker.isBanner3 = false;\n      this.checker.isClientCover = false;\n\n      this.options.videoLink = \"\";\n      this.options.about = \"\";\n      this.options.banner1 = \"\";\n      this.options.banner2 = \"\";\n      this.options.banner3 = \"\";\n      this.options.client_file = \"\";\n      this.options.client_satisfait = \"\";\n\n      this.viewer.imgBan1 = \"\";\n      this.viewer.imgBan2 = \"\";\n      this.viewer.imgBan3 = \"\";\n      this.viewer.imgLogo = \"\";\n      this.viewer.client_cover = \"\";\n      this.checker.isLogo = false;\n    },\n\n    submitted(strOpt, option) {\n      if (option === \"\") {\n        this.$swal({\n          showConfirmButton: false,\n          timer: 2000,\n          timerProgressBar: false,\n          icon: \"warning\",\n          text: \"Aucune information dectectée pour l'envoi de données au serveur!\",\n        });\n        return;\n      }\n      let formData = new FormData();\n      formData.append(`${strOpt}`, option);\n      this.$swal({\n        buttonsStyling: true,\n        title: \"\",\n        text: \"Voulez-vous vraiment personnaliser cette section de votre site web ?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\",\n      }).then((result) => {\n        if (result.value) {\n          this.loading = true;\n          this.$axios\n            .post(\"admin/personnalisation\", formData)\n            .then((result) => {\n              let status = result.data.reponse.status;\n              console.log(status);\n              this.loading = false;\n\n              if (status === \"success\") {\n                this.loading = false;\n                this.$swal({\n                  showConfirmButton: false,\n                  timer: 2000,\n                  timerProgressBar: false,\n                  icon: \"success\",\n                  text: \"La personnalisation du site web est effectuée avec succès!\",\n                });\n                this.cancel();\n              }\n            })\n            .catch((err) => {\n              console.log(err);\n              this.loading = false;\n            });\n        } else return;\n      });\n    },\n\n    saveBanner() {\n      let formData = new FormData();\n      if (this.options.banner1 !== \"\") {\n        formData.append(\"banner_img1\", this.options.banner1);\n        this.options.banner2 = \"\";\n        this.options.banner3 = \"\";\n      } else if (this.options.banner2 !== \"\") {\n        formData.append(\"banner_img2\", this.options.banner2);\n        this.options.banner1 = \"\";\n        this.options.banner3 = \"\";\n      } else if (this.options.banner3 !== \"\") {\n        formData.append(\"banner_img3\", this.options.banner3);\n        this.options.banner1 = \"\";\n        this.options.banner2 = \"\";\n      } else {\n        this.$swal({\n          showConfirmButton: false,\n          timer: 2000,\n          timerProgressBar: false,\n          icon: \"warning\",\n          text: \"vous devez charger une image bannière !\",\n        });\n        return;\n      }\n      this.$swal({\n        buttonsStyling: true,\n        title: \"\",\n        text: \"Voulez-vous vraiment personnaliser cette section de votre site web ?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\",\n      }).then((result) => {\n        if (result.value) {\n          this.loading = true;\n\n          this.$axios\n            .post(\"admin/personnalisation\", formData)\n            .then((result) => {\n              console.log(JSON.stringify(result.data));\n              let status = result.data.reponse.status;\n\n              this.loading = false;\n              if (status === \"success\") {\n                this.loading = false;\n                this.$swal({\n                  showConfirmButton: false,\n                  timer: 2000,\n                  timerProgressBar: false,\n                  icon: \"success\",\n                  text: \"La personnalisation du site web est effectuée avec succès!\",\n                });\n                this.cancel();\n              }\n            })\n            .catch((err) => {\n              console.log(err);\n              this.loading = false;\n            });\n        } else return;\n      });\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}