-if=\"realisation.file2 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file2.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 2</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file3\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage3($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file3 !== ''\"\n                          class=\"custom-file-label text-blue\"\n                          for=\"InputFile\"\n                          >{{ realisation.file3.name }}</label\n                        >\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 3</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file4\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage4($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"realisation.file4 !=