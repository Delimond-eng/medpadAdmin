CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"video_create_page.vue","sourceRoot":"src/components/pages","sourcesContent":["<template>\n  <div>\n    <!-- Content Header (Page header) -->\n    <div class=\"content-header\">\n      <div class=\"container-fluid\">\n        <div class=\"row mb-2\">\n          <div class=\"col-sm-6\">\n            <h1 class=\"m-0 text-dark\">Ajout des vidéos</h1>\n          </div>\n          <div class=\"col-sm-6\">\n            <ol class=\"breadcrumb float-sm-right\">\n              <li class=\"breadcrumb-item\">\n                <router-link :to=\"{ name: 'home' }\" tag=\"a\">Acceuil</router-link>\n              </li>\n            </ol>\n          </div>\n          <!-- /.col -->\n        </div>\n        <!-- /.row -->\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content-header -->\n\n    <!-- Main content -->\n    <div class=\"content\">\n      <div class=\"container-fluid\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Lien de Youtube</h3>\n              </div>\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" enctype=\"multipart/form-data\">\n                <div class=\"card-body\" id=\"contentFields\">\n                  <div class=\"row\">\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 1</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[0]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 2</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[1]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 3</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[2]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n                  </div>\n\n                  <div class=\"row\">\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 4</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[3]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 5</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[4]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 6</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[5]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n                  </div>\n\n                  <div class=\"row\">\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 7</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[6]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 8</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[7]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 9</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[8]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n                  </div>\n\n                  <div class=\"row\">\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 10</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[9]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 11</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[10]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n\n                    <div class=\"col-12 col-md-6 col-lg-4\">\n                      <div class=\"form-group\">\n                        <label for=\"InputTitle\">Lien vidéo 12</label>\n                        <input\n                          type=\"text\"\n                          v-model=\"liens[12]\"\n                          class=\"form-control border-primary\"\n                          id=\"InputTitle\"\n                          placeholder=\"Entrer le lien youtube de la vidéo\"\n                        />\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button type=\"button\" class=\"btn btn-primary mr-2\" @click=\"addVideo\">\n                  <i class=\"fa fa-plus mr-2\"></i> Ajouter\n                </button>\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <table class=\"table table-striped table-valign-middle\">\n                  <thead>\n                    <tr>\n                      <th>Lien vidéo</th>\n                      <th></th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"(data, index) in videos\" :key=\"index\">\n                      <td v-if=\"data != ''\">\n                        {{ data }}\n                      </td>\n                      <td v-if=\"data != ''\">\n                        <button\n                          type=\"button\"\n                          class=\"btn btn-sm btn-danger\"\n                          @click=\"onDelete(index)\"\n                        >\n                          <i class=\"fa fa-trash mr-2\"></i>supprimer\n                        </button>\n                      </td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"videoLink\",\n\n  data() {\n    return {\n      videos: [],\n      liens: [],\n    };\n  },\n\n  mounted() {\n    this.viewPersonalization();\n    console.log(\"source :\" + this.src);\n  },\n\n  methods: {\n    onDelete(link) {\n      let formData = new FormData();\n      console.log(link);\n      formData.append(\"index\", link);\n      this.$swal({\n        buttonsStyling: true,\n        title: \"\",\n        text: \"Voulez-vous vraiment supprimer définitivement le lien de cette vidéo ?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\",\n      }).then((result) => {\n        if (result.value) {\n          this.$axios\n            .post(\"admin/personnalisation/supprimervideo\", formData)\n            .then((result) => {\n              let status = result.data.reponse.status;\n              console.log(status);\n\n              if (status === \"success\") {\n                this.isLoad = false;\n                this.$swal({\n                  showConfirmButton: false,\n                  timer: 2000,\n                  timerProgressBar: false,\n                  icon: \"success\",\n                  text: \"la suppression est effectuée avec succès!\",\n                });\n\n                this.viewPersonalization();\n              }\n            })\n            .catch((err) => console.log(err));\n        } else return;\n      });\n    },\n    viewPersonalization() {\n      this.$axios\n        .get(\"admin/personnalisation/get\")\n        .then((result) => {\n          this.videos = result.data.personalisation.video_presentation;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n\n    addVideo() {\n      let formData = new FormData();\n      let video = \"\";\n      for (let i = 0; i < this.liens.length; i++) {\n        if (this.liens[i] != \"\") {\n          if (video === \"\") {\n            video = this.liens[i];\n            continue;\n          }\n          video = video + \"@@@\" + this.liens[i];\n        } else {\n          this.$swal({\n            showConfirmButton: false,\n            timer: 2000,\n            timerProgressBar: false,\n            icon: \"warning\",\n            text: \"veuillez entrer les liens youtube de vos vidéos!\",\n          });\n          return;\n        }\n      }\n      formData.append(\"video_presentation\", video);\n      this.$axios.post(\"admin/personnalisation\", formData).then((res) => {\n        console.log(JSON.stringify(res.data));\n        let status = res.data.reponse.status;\n\n        if (status === \"success\") {\n          this.$swal({\n            showConfirmButton: false,\n            timer: 2000,\n            timerProgressBar: false,\n            icon: \"success\",\n            text: \"la suppression est effectuée avec succès!\",\n          });\n          this.viewPersonalization();\n          this.cancel();\n        }\n      });\n    },\n    cancel() {\n      this.liens.delete();\n    },\n  },\n};\n</script>\n"]}]}