CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"catalog_create_page.vue","sourceRoot":"src/components/pages","sourcesContent":["<template>\n  <div>\n    <!-- Content Header (Page header) -->\n    <div class=\"content-header\">\n      <div class=\"container-fluid\">\n        <div class=\"row mb-2\">\n          <div class=\"col-sm-6\">\n            <h1 class=\"m-0 text-dark\">Création Catalogue</h1>\n          </div>\n          <!-- /.col -->\n          <div class=\"col-sm-6\">\n            <ol class=\"breadcrumb float-sm-right\">\n              <li class=\"breadcrumb-item\">\n                <router-link :to=\"{ name: 'home' }\" tag=\"a\">Acceuil</router-link>\n              </li>\n            </ol>\n          </div>\n        </div>\n        <!-- /.row -->\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content-header -->\n\n    <!-- Main content -->\n    <div class=\"content\">\n      <div class=\"container-fluid\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card card-primary\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\">Création</h3>\n              </div>\n\n              <!-- /.card-header -->\n              <!-- form start -->\n              <form role=\"form\" enctype=\"multipart/form-data\">\n                <div class=\"card-body\">\n                  <div class=\"form-group\">\n                    <label for=\"InputTitle\">Texte descriptif </label>\n                    <input\n                      type=\"email\"\n                      v-model=\"catalog.title\"\n                      class=\"form-control border-primary\"\n                      id=\"InputTitle\"\n                      placeholder=\"Entrer le titre de la réalisation\"\n                    />\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"InputFile\">Images de catalogue</label>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file1\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage1($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"catalog.file1 !== ''\"\n                          class=\"custom-file-label text-bold text-green\"\n                          for=\"InputFile\"\n                          >{{ catalog.file1.name }}</label\n                        >\n\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 1</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file2\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage2($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"catalog.file2 !== ''\"\n                          class=\"custom-file-label text-bold text-green\"\n                          for=\"InputFile\"\n                          >{{ catalog.file2.name }}</label\n                        >\n\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 2</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file3\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage3($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"catalog.file3 !== ''\"\n                          class=\"custom-file-label text-bold text-green\"\n                          for=\"InputFile\"\n                          >{{ catalog.file3.name }}</label\n                        >\n\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 3</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file4\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage4($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"catalog.file4 !== ''\"\n                          class=\"custom-file-label text-bold text-green\"\n                          for=\"InputFile\"\n                          >{{ catalog.file4.name }}</label\n                        >\n\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 4</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file5\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage5($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"catalog.file5 !== ''\"\n                          class=\"custom-file-label text-bold text-green\"\n                          for=\"InputFile\"\n                          >{{ catalog.file5.name }}</label\n                        >\n\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 5</label\n                        >\n                      </div>\n                    </div>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file6\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadImage6($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"catalog.file6 !== ''\"\n                          class=\"custom-file-label text-bold text-green\"\n                          for=\"InputFile\"\n                          >{{ catalog.file6.name }}</label\n                        >\n\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir image 6</label\n                        >\n                      </div>\n                    </div>\n                  </div>\n\n                  <div class=\"form-group\">\n                    <label for=\"InputFile\">Image de couverture</label>\n\n                    <div class=\"input-group mb-2\">\n                      <div class=\"custom-file\">\n                        <input\n                          type=\"file\"\n                          ref=\"file7\"\n                          class=\"custom-file-input border-primary\"\n                          @change=\"uploadCover($event)\"\n                          multiple=\"multiple\"\n                          id=\"InputFile\"\n                        />\n                        <label\n                          v-if=\"catalog.coverImage !== ''\"\n                          class=\"custom-file-label text-bold text-green\"\n                          for=\"InputFile\"\n                          >{{ catalog.coverImage.name }}</label\n                        >\n\n                        <label v-else class=\"custom-file-label\" for=\"InputFile\"\n                          >Choisir de couverture</label\n                        >\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </form>\n              <!-- /.card-body -->\n\n              <div class=\"card-footer\">\n                <button type=\"button\" class=\"btn btn-primary mr-2\" @click=\"submitted\">\n                  <i class=\"fa fa-plus mr-2\" v-if=\"isLoad === false\"></i\n                  ><span\n                    class=\"spinner-border spinner-border-sm mr-2\"\n                    v-if=\"isLoad === true\"\n                  ></span\n                  >Poster\n                </button>\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"cancel\">\n                  <i class=\"fa fa-circle-notch mr-2\"></i> Annuler\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h2 class=\"card-title\">Catalogue</h2>\n              </div>\n              <div class=\"card-body\">\n                <div class=\"row\">\n                  <div\n                    class=\"col-md-2\"\n                    v-for=\"(img, index) in catalogs.images\"\n                    :key=\"index\"\n                  >\n                    <div>\n                      <img\n                        class=\"img-fluid m-1\"\n                        :src=\"img\"\n                        style=\"\n                          width: 100%;\n                          height: 200px;\n                          object-fit: cover;\n                          object-position: center;\n                        \"\n                      />\n                      <button\n                        class=\"btn btn-danger\"\n                        style=\"\n                          position: absolute;\n                          z-index: 1000;\n                          left: 0;\n                          right: 0;\n                          top: 80px;\n                          margin-left: auto;\n                          margin-right: auto;\n                        \"\n                        @click=\"onDelete(index + 1)\"\n                      >\n                        <i class=\"fa fa-trash\"></i>\n                      </button>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"rcatalogComponent\",\n  data() {\n    return {\n      catalog: {\n        title: \"\",\n        file1: \"\",\n        file2: \"\",\n        file3: \"\",\n        file4: \"\",\n        file5: \"\",\n        file6: \"\",\n        coverImage: \"\",\n      },\n      isUploaded: false,\n      isLoad: false,\n      catalogs: {},\n    };\n  },\n\n  mounted() {\n    this.viewCatalogs();\n  },\n\n  methods: {\n    submitted() {\n      if (this.isUploaded === false) {\n        this.$swal({\n          toast: true,\n          showConfirmButton: false,\n          position: \"top-end\",\n          timer: 2000,\n          timerProgressBar: false,\n          icon: \"warning\",\n          title: \"vous devez charger au moins une image du catalogue!\",\n        });\n        return;\n      }\n      this.isLoad = true;\n      let formData = new FormData();\n      formData.append(\"texte\", this.catalog.title);\n      formData.append(\"image_1\", this.catalog.file1);\n      formData.append(\"image_2\", this.catalog.file2);\n      formData.append(\"image_3\", this.catalog.file3);\n      formData.append(\"image_4\", this.catalog.file4);\n      formData.append(\"image_5\", this.catalog.file5);\n      formData.append(\"image_6\", this.catalog.file6);\n      formData.append(\"image_couverture\", this.catalog.coverImage);\n\n      this.$axios\n        .post(\"admin/projets/gamme\", formData)\n        .then((result) => {\n          console.log(JSON.stringify(result.data));\n          let status = result.data.reponse.status;\n          console.log(status);\n\n          if (status === \"success\") {\n            this.isLoad = false;\n            this.$swal({\n              showConfirmButton: false,\n              timer: 2000,\n              timerProgressBar: false,\n              icon: \"success\",\n              text: \"Le catalogue a été ajouté avec succès !\",\n            });\n            this.cancel();\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n          this.isLoad = false;\n          this.cancel();\n        });\n    },\n\n    viewCatalogs() {\n      try {\n        this.$axios\n          .get(\"admin/personnalisation/get\")\n          .then((result) => {\n            this.catalogs = result.data.personalisation.gamme;\n          })\n          .catch((err) => console.log(err));\n      } catch (err) {\n        console.log(err);\n      }\n    },\n\n    cancel() {\n      this.catalog.title = \"\";\n      this.catalog.image = \"\";\n      this.catalog.file1 = \"\";\n      this.catalog.file2 = \"\";\n      this.catalog.file3 = \"\";\n      this.catalog.file4 = \"\";\n      this.catalog.file5 = \"\";\n      this.catalog.file6 = \"\";\n      this.catalog.coverImage = \"\";\n      this.isUploaded = false;\n    },\n\n    onDelete(id) {\n      console.log(id);\n      let formData = new FormData();\n      formData.append(`image_${id}`, id);\n      this.$swal({\n        buttonsStyling: true,\n        title: \"Etes vous sûr?\",\n        text: \"de vouloir supprimer définitivement l'image de la gamme ?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\",\n      }).then((result) => {\n        if (result.value) {\n          this.$axios.post(\"admin/projets/gamme/supprimer\", formData).then((result) => {\n            var status = result.data.reponse.status;\n            if (status == \"success\") {\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"top-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"success\",\n                title: \"la suppression est effectuée!\",\n              });\n              this.viewCatalogs();\n            } else {\n              this.$swal({\n                toast: true,\n                showConfirmButton: false,\n                position: \"bottom-end\",\n                timer: 2000,\n                timerProgressBar: false,\n                icon: \"warning\",\n                title: \"Echec de la suppression !\",\n              });\n              return;\n            }\n          });\n        } else return;\n      });\n    },\n\n    uploadImage1() {\n      this.catalog.file1 = this.$refs.file1.files[0];\n      console.log(this.catalog.file1);\n      this.isUploaded = true;\n    },\n    uploadImage2() {\n      this.catalog.file2 = this.$refs.file2.files[0];\n      console.log(this.catalog.file2);\n      this.isUploaded = true;\n    },\n\n    uploadImage3() {\n      this.catalog.file3 = this.$refs.file3.files[0];\n      console.log(this.catalog.file3);\n      this.isUploaded = true;\n    },\n\n    uploadImage4() {\n      this.catalog.file4 = this.$refs.file4.files[0];\n      console.log(this.catalog.file4);\n      this.isUploaded = true;\n    },\n    uploadImage5() {\n      this.catalog.file5 = this.$refs.file5.files[0];\n      console.log(this.catalog.file5);\n      this.isUploaded = true;\n    },\n    uploadImage6() {\n      this.catalog.file6 = this.$refs.file6.files[0];\n      console.log(this.catalog.file6);\n      this.isUploaded = true;\n    },\n\n    uploadCover() {\n      this.catalog.coverImage = this.$refs.file7.files[0];\n      console.log(this.catalog.coverImage);\n      this.isUploaded = true;\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}