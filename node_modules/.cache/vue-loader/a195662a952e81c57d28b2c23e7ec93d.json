t -->\n    <div class=\"content\">\n      <div class=\"container-fluid\">\n        <div class=\"row\">\n          <!-- ./col -->\n          <div class=\"col-lg-3 col-6\">\n            <!-- small box -->\n            <div class=\"small-box bg-warning\">\n              <div class=\"inner\">\n                <h3>{{ data.services.length }}</h3>\n\n                <p>Services en ligne</p>\n              </div>\n              <div class=\"icon\">\n                <i class=\"ion ion-stats-bars\"></i>\n              </div>\n              <a href=\"#\" class=\"small-box-footer\"\n                >En savoir plus <i class=\"fas fa-arrow-circle-right\"></i\n              ></a>\n            </div>\n          </div>\n          <!-- ./col -->\n          <div class=\"col-lg-3 col-6\">\n            <!-- small box -->\n            <div class=\"small-box bg-success\">\n              <div class=\"inner\">\n                <h3>{{ data.projects.length }}</h3>\n\n                <p>Tous nos projets en ligne</p>\n              </div>\n              <div class=\"icon\">\n                <i class=\"ion ion-pie-graph\"></i>\n              </div>\n              <a href=\"#\" class=\"small-box-footer\"\n                >En savoir plus <i class=\"fas fa-arrow-circle-right\"></i\n              ></a>\n            </div>\n          </div>\n          <!-- ./col -->\n\n          <!-- ./col -->\n          <div class=\"col-lg-3 col-6\">\n            <!-- small box -->\n            <div class=\"small-box bg-primary\">\n              <div class=\"inner\">\n                <h3>{{ data.avantages.length }}</h3>\n\n                <p>Nos Avantages en ligne</p>\n              </div>\n              <div class=\"icon\">\n                <i class=\"ion ion-pie-graph\"></i>\n              </div>\n              <a href=\"#\" class=\"small-box-footer\"\n                >En savoir plus <i class=\"fas fa-arrow-circle-right\"></i\n              ></a>\n            </div>\n          </div>\n          <!-- ./col -->\n\n          <!-- ./col -->\n          <div class=\"col-lg-3 col-6\">\n            <!-- small box -->\n            <div class=\"small-box bg-danger\">\n              <div class=\"inner\">\n                <h3>{{ data.gamme.length }}</h3>\n\n                <p>Gamme images en ligne</p>\n              </div>\n              <div class=\"icon\">\n                <i class=\"ion ion-pie-graph\"></i>\n              </div>\n              <a href=\"#\" class=\"small-box-footer\"\n                >En savoir plus <i class=\"fas fa-arrow-circle-right\"></i\n              ></a>\n            </div>\n          </div>\n          <!-- ./col -->\n        </div>\n      </div>\n      <!-- /.container-fluid -->\n    </div>\n    <!-- /.content -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"home\",\n\n  data() {\n    return {\n      data: {\n        projects: [],\n        services: [],\n        gamme: [],\n        avantages: [],\n      },\n    };\n  },\n\n  mounted() {\n    this.viewDatas();\n  },\n\n  methods: {\n    viewDatas() {\n      try {\n        this.$axios.get(\"admin/contenus\").then((res) => {\n          this.data.projects = res.data.contenus.projets;\n          this.data.services = res.data.contenus.services;\n        });\n\n        this.$axios.get(\"admin/personnalisation/get\").then((res) => {\n          this.data.gamme = res.data.personalisation.gamme.images;\n          this.data.avantages = res.data.personalisation.avantages;\n        });\n      } catch (err) {\n        console.log(err);\n      }\n    },\n  },\n};\n</script>\n"]}]}